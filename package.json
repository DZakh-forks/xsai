{
  "name": "xsai",
  "version": "0.0.3",
  "type": "module",
  "author": "Ëóç+85CD",
  "license": "MIT",
  "homepage": "https://github.com/moeru-ai/xsai",
  "repository": "github:moeru-ai/xsai",
  "bugs": "https://github.com/moeru-ai/xsai/issues",
  "description": "Extra-small AI SDK for any OpenAI-compatible API.",
  "keywords": [
    "ai",
    "openai",
    "ollama"
  ],
  "packageManager": "pnpm@9.14.2",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll",
    "build:watch": "pkgroll --watch",
    "bump": "bumpp --no-push",
    "lint": "eslint --flag unstable_ts_config .",
    "lint:fix": "eslint --flag unstable_ts_config . --fix",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.2",
    "@importantimport/eslint-config": "1.0.0-alpha.2",
    "@importantimport/tsconfig": "^0.1.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.9.1",
    "automd": "^0.3.12",
    "bumpp": "^9.8.1",
    "eslint": "^9.15.0",
    "jiti": "^2.4.0",
    "pkgroll": "^2.5.1",
    "vitest": "^2.1.5"
  }
